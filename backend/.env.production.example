# Production Environment Variables for UMKM Radar MRT
# Copy this file to .env.production and update the values

# Database
DATABASE_URL=postgres://postgres:CHANGE_THIS_PASSWORD@db:5432/umkmradar

# Redis
REDIS_URL=redis://redis:6379

# MinIO Configuration
MINIO_ENDPOINT=minio
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=CHANGE_THIS_ACCESS_KEY
MINIO_SECRET_KEY=CHANGE_THIS_SECRET_KEY
MINIO_BUCKET=assets

# Public URL for assets (served via Nginx)
PUBLIC_ASSET_URL=https://assets.pengaruh.my.id

# Backend API URL
API_URL=https://api.pengaruh.my.id

# CORS Origins (comma-separated)
CORS_ORIGINS=https://umkmradar.pengaruh.my.id,https://dashboard.pengaruh.my.id,https://*.pengaruh.my.id

# Node Environment
NODE_ENV=production

# Security
JWT_SECRET=CHANGE_THIS_JWT_SECRET
SESSION_SECRET=CHANGE_THIS_SESSION_SECRET
